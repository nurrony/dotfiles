[cmd_duration]
min_time = 500

[aws]
format = 'on [$symbol($profile )(\($region\) )(\[$duration\])]($style)'
style = "bold green"
symbol = " "

[battery]
disabled = true

[dart]
symbol = " "
format = "via [$version](bold green) "

[directory]
truncation_length = 2
read_only = " "

[docker_context]
symbol = " "
only_with_files = true
disabled = false

[git_branch]
symbol = " "

[git_commit]
commit_hash_length = 8

[golang]
symbol = " "

[helm]
format = 'via [$symbol]($style)'

[java]
symbol = " "
style = "bold green"

[kubernetes]
format = 'on [$symbol]($style)'

[memory_usage]
symbol = " "

[package]
display_private = true

[scala]
symbol = " "

[python]
symbol = " "

[ruby]
symbol = " "

[rust]
symbol = " "

[username]
style_user = "white bold"
style_root = "black bold"
format = "($style) "
disabled = false
show_always = true
